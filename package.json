{
  "name": "mantine-next-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 1764",
    "analyze": "ANALYZE=true next build",
    "build": "next build && echo 'ui.mantine.dev' > out/CNAME && touch out/.nojekyll",
    "deploy": "npm run build && gh-pages -d out",
    "start": "next start",
    "typecheck": "tsc --noEmit",
