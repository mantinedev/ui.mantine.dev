    "prettier:write": "prettier --write \"**/*.{ts,tsx,css}\"",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "test": "npm run prettier:check && npm run lint && npm run typecheck && npm run jest",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
